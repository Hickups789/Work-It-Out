        <div id="selectType">
            <h3 class="h3 text-center">Workout</h3>
        </div>
        <div class="d-flex justify-content-center" id="workout">
            <table class="table text-center">
                <thead>
                <tr>
                    <th scope="col">Exercise</th>
                    <th scope="col"># of Reps</th>
                    <th scope="col"># of Sets</th>
                </tr>
                </thead>
                <tbody>
                    {{#each workout.exercise as |exercise|}}
                        <tr>
                            <th scope="row">{{workout.exercise.exercise_name}}</th>
                            <td>{{workout.exercise.num_sets}}</td>
                            <td>{{workout.exercise.num.reps}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>