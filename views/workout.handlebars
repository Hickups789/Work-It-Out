        <div id="selectType">
            <h3 class="h3 text-center">{{workout.workout_name}}</h3>
        </div>
        <div class="d-flex justify-content-center" id="workout">
            <table class="table text-center">
                <thead>
                <tr>
                    <th scope="col">Exercise</th>
                    <th scope="col"># of Reps</th>
                    <th scope="col"># of Sets</th>
                </tr>
                </thead>
                <tbody>
                    {{#each exercises as |exercise|}}
                        <tr>
                            <td scope="row">{{ exercise.exercise_name }}</td>
                            <td>{{exercise.num_sets}}</td>
                            <td>{{exercise.num_reps}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>